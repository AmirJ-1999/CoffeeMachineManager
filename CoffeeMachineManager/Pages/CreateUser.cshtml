@page
@model CoffeeMachineManager.Pages.CreateUserModel

<h1>Create User</h1>

<form method="post">
    <div>
        <label for="Name">Name</label>
        <input type="text" id="Name" name="NewUser.Name" />
    </div>
    <div>
        <label for="Email">Email</label>
        <input type="email" id="Email" name="NewUser.Email" />
    </div>
    <div>
        <label for="Role">Role</label>
        <input type="text" id="Role" name="NewUser.Role" />
    </div>
    <button type="submit">Create</button>
</form>
